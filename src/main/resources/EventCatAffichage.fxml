<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.EventCategorieAffichageC">
    <children>
        <VBox prefHeight="404.0" prefWidth="600" spacing="10" AnchorPane.bottomAnchor="6.0" AnchorPane.leftAnchor="10" AnchorPane.rightAnchor="10" AnchorPane.topAnchor="10">
            <children>
                <TableView fx:id="tableView">
                    <columns>
                        <TableColumn fx:id="nomColumn" prefWidth="150" text="Nom" />
                        <TableColumn fx:id="descriptionColumn" prefWidth="200" text="Description" />
                        <TableColumn fx:id="lieuColumn" prefWidth="150" text="Lieu" />
                        <TableColumn fx:id="dateColumn" prefWidth="150" text="Date" />
                        <TableColumn fx:id="categorieColumn" prefWidth="150" text="Categorie" />
                    </columns>
                </TableView>
            <Button fx:id="exporter" mnemonicParsing="false" onAction="#exporter" prefHeight="34.0" prefWidth="61.0" text="exporter" />
            </children>
        </VBox>
    </children>
</AnchorPane>
