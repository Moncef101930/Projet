<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tn.esprit.event.controller.TicketListController">
    <children>
        <VBox layoutX="20" layoutY="20" prefHeight="560" prefWidth="760" spacing="10">
            <children>
                <TableView fx:id="ticketTable">
                    <columns>
                        <TableColumn fx:id="idColumn" prefWidth="50" text="ID" />
                        <TableColumn fx:id="utilisateurColumn" prefWidth="150" text="Utilisateur" />
                        <TableColumn fx:id="evenementColumn" prefWidth="150" text="Evenement" />
                        <TableColumn fx:id="dateColumn" prefWidth="150" text="Date Achat" />
                        <TableColumn fx:id="typeColumn" prefWidth="150" text="Type Ticket" />
                    </columns>
                </TableView>
                <Button fx:id="btnDelete" onAction="#handleDelete" prefHeight="30" prefWidth="100" text="Delete" />
            <Button onAction="#LogoutFromticketlist" prefHeight="30" prefWidth="100" text="Logout" />
            </children>
        </VBox>
    </children>
</AnchorPane>
